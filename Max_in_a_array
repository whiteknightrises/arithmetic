package java_strings;
import java.util.*;

public class array {
	Scanner sc = new Scanner(System.in);
	void arr() {
		int a[]=new int[5];
		int max=0;
		for(int i=0;i<a.length;i++) {
			System.out.println("enter");
			a[i]=sc.nextInt();
		}
		for(int i=0;i<a.length;i++) {
			if(a[i]>max)
				max=a[i];
		}
		
		System.out.println(max);
		
	}

	public static void main(String[] args) {
		array a = new array();
		a.arr();
	}
}
