package java_strings;
import java.util.*;

public class reverse_array {
	

static int max(int ...a) {
	if(a.length==0)
		return Integer.MIN_VALUE;
	int m=a[0];
	for(int i=0;i<a.length;i++) {
		if(a[i]>m)
			m=a[i];
	}
	return m;
}
	

	public static void main(String[] args) {
		
		int a[]= {5,4,3,2,1,55};
		
		System.out.println(max(a));
	}
}
	

